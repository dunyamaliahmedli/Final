@model Team
@{
    ViewData["Title"] = "Create";
}

<form class="container" method="post" enctype="multipart/form-data">
    <div class="mb-3">
        <label asp-for="Photo" class="form-label"></label>
        <input asp-for="Photo" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="FullName" class="form-label"></label>
        <input asp-for="FullName" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="JobName" class="form-label"></label>
        <input asp-for="JobName" class="form-control">
    </div>


    <button style="z-index=10;" class="btn pb-140 btn-secondary" type="submit">Create</button>

</form>

